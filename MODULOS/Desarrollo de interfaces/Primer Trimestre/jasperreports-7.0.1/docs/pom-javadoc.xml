<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>net.sf.jasperreports</groupId>
		<artifactId>jasperreports-parent</artifactId>
		<version>${revision}</version>
		<relativePath>../pom-parent.xml</relativePath>
	</parent>
	<artifactId>jasperreports-javadoc</artifactId>
	<packaging>pom</packaging>
	<name>JasperReports Javadoc</name>
	<description>JasperReports Javadoc</description>
	<url>http://jasperreports.sourceforge.net</url>
	<scm>
		<connection>${scmConnection}</connection>
		<tag>${revision}</tag>
		<url>${scmUrl}</url>
	</scm>
	<properties>
		<maven.javadoc.skip>false</maven.javadoc.skip>
	</properties>
	<modules>
		<!-- 
		<module>tools/metadata</module>
		<module>tools/annotation-processors</module>
		<module>ext/fonts</module>
		-->
		<module>../core</module>
		<!--
		<module>pom-javaflow.xml</module>
		-->
		<module>../ext/ant</module>
		<module>../ext/barbecue</module>
		<module>../ext/barcode4j</module>
		<module>../ext/castor</module>
		<module>../ext/charts</module>
		<module>../ext/chart-customizers</module>
		<module>../ext/chart-themes</module>
		<module>../ext/chrome</module>
		<module>../ext/data-adapters</module>
		<module>../ext/data-adapters-http</module>
		<module>../ext/ejbql</module>
		<module>../ext/ejbql-j2ee</module>
		<module>../ext/excel-poi</module>
		<module>../ext/fastexcel</module>
		<module>../ext/functions</module>
		<module>../ext/google-maps</module>
		<module>../ext/groovy</module>
		<module>../ext/hibernate</module>
		<module>../ext/hibernate-j2ee</module>
		<module>../ext/interactivity</module>
		<module>../ext/javascript</module>
		<module>../ext/jaxen</module>
		<module>../ext/jdt</module>
		<module>../ext/json</module>
		<module>../ext/olap</module>
		<module>../ext/pdf</module>
		<module>../ext/servlets</module>
		<module>../ext/servlets-j2ee</module>
		<module>../ext/sort</module>
		<module>../ext/spring</module>
		<module>../ext/velocity</module>
		<module>../ext/xalan</module>
	</modules>
	<reporting>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>${maven-javadoc-plugin.version}</version>
				<configuration>
					<source>8</source>
					<quiet>true</quiet>
					<doclint>none</doclint>
				</configuration>
				<reportSets>
					<reportSet>
						<id>aggregate</id>
						<inherited>false</inherited>
						<reports>
							<report>aggregate</report>
						</reports>
					</reportSet>
					<reportSet>
						<id>javadoc</id>
						<reports>
							<report>javadoc</report>
						</reports>
					</reportSet>
				</reportSets>
			</plugin>
		</plugins>
	</reporting>
</project>
